{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:esolitos/laravel-foundation.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:esolitos/laravel-acl.git"
        }
    ],
	"require": {
		"laravel/framework": "4.1.*",
        "laravelbook/ardent": "2.4.*",
        "felixkiss/uniquewith-validator": "1.0.*",
        "vespakoen/menu": "2.0.*",
        "vivify-ideas/acl": "dev-master"
	},
    "require-dev": {
        "way/generators": "2.*",
        "fzaninotto/faker": "v1.3.0",
        "phpunit/phpunit": "4.1.*",
        "esolitos/laravel-foundation": "0.1.*"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
